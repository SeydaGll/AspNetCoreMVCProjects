@model List<Movie>


<table class="table table-hover">
    <thead>
        <tr>
            <th>Title</th>
            <th>Price</th>
            <th>Genre</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Movie movie in Model)
        {
            <tr>
                <td>@movie.Title</td>
                <td>@movie.Price</td>
                <td>@movie.Genre</td>
            </tr>
        }
        
    </tbody>
    
</table>